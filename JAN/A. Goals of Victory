#include<bits/stdc++.h>
using namespace std;

int main(){
    cin.tie(nullptr);
    ios::sync_with_stdio(false);
    
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int x;
         int sum = 0;
        for(int i = 0;i<n-1;i++){
            cin>>x;
           sum= sum+ x;
            }
        
        cout<<-(sum)<<endl;
    }
    return 0;
    
}